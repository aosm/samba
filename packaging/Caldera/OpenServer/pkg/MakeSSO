:
# MakeSSO
#

rm -rf archives sso

    CDMT_DIR=`pwd`; export CDMT_DIR
    Samba_DIR=`pwd`; export Samba_DIR
    cdmtParse
    if test $? != 0
    then
    exit 1
    fi

    cdmtCompress
    if test $? != 0
    then
    exit 1
    fi

    cdmtArchive
    if test $? != 0
    then
    exit 1
    fi
